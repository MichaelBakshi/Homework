static double ShumAhezka (params int [] numbers)
        {
            double sum = 0;

            for (int i = 0; i < numbers.Length; i++)
            {
                //sum += Math.Pow(numbers[i], i+1);

                //numbers[i] * numbers[i] * hezka;
                int hezka = i+1;
                int number = numbers[i];
                int powSum = 0;

                for (int n = 1; n <= hezka; n++)
                {
                    powSum += number * number;
                }

                int sum = 
                //numbers [i] = i * numbers[i]*numbers[i];
            }

            return sum;
        }


        static void Main(string[] args)
        {

            Console.WriteLine(  ShumAhezka(2, 3));
        }
