{
        static void OutAB (out int a, out int b)
        {
             a = Convert.ToInt32(Console.ReadLine());
             b = Convert.ToInt32(Console.ReadLine());

        }
        static void Main(string[] args)
        {
            int a, b;
            OutAB(out a, out b);

            Console.WriteLine(a);
            Console.WriteLine(b);

            Console.ReadLine();
        }
    }
